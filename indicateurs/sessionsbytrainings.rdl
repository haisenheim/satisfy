  <DataSets>
    <DataSet Name="DST01">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 100*count(*)/(select count(*) from formations f join sessions s on f.id=s.formation_id) pourcentage,  intitule  from formations f join sessions s on f.id = s.formation_id group by formation_id
union
select 0 pourcentage, intitule from formations
order by pourcentage ;</CommandText>
      </Query>
      <Fields>
        <Field Name="pourcentage">
          <DataField>pourcentage</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="intitule">
          <DataField>intitule</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
